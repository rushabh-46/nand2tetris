class BubbleSort {
    function int S(Array a,int size) {
        var int i,j,temp;
        let i=0;
        while(i<size-1) {
            let j=0;
            while(j<size-i-1) {
                if(a[j]>a[j+1]) {
                    let temp = a[j];
                    let a[j] = a[j+1];
                    let a[j+1] = temp;
                }
                let j=j+1;
            }
            let i=i+1;
        }
    return 0;
    }
}